<Dictionary xmlns="http://www.opengis.net/gml/3.2" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" gml:id="GypsumFormsValue" xmlns:gml="http://www.opengis.net/gml/3.2" xsi:schemaLocation="http://www.opengis.net/gml/3.2 http://schemas.opengis.net/gml/3.2.1/gml.xsd">
  <identifier codeSpace="http://www.fao.org/global-soil-partnership/dictionary/GypsumFormsValue.xml">Classification of forms of secondary gypsum</identifier>
  <dictionaryEntry>
    <Definition gml:id="GypsumFormsValue_SC">
      <description>soft concretions</description>
      <identifier codeSpace="http://www.fao.org/global-soil-partnership/dictionary/GypsumFormsValue.xml">SC</identifier>
    </Definition>
  </dictionaryEntry>
  <dictionaryEntry>
    <Definition gml:id="GypsumFormsValue_D">
      <description>disperse powdery gypsum</description>
      <identifier codeSpace="http://www.fao.org/global-soil-partnership/dictionary/GypsumFormsValue.xml">D</identifier>
    </Definition>
  </dictionaryEntry>
  <dictionaryEntry>
    <Definition gml:id="GypsumFormsValue_G">
      <description>“gazha” (clayey water-saturated layer with high gypsum content)</description>
      <identifier codeSpace="http://www.fao.org/global-soil-partnership/dictionary/GypsumFormsValue.xml">G</identifier>
    </Definition>
  </dictionaryEntry>
  <dictionaryEntry>
    <Definition gml:id="GypsumFormsValue_HL">
      <description>hard cemented layer or layers of gypsum (less than 10 cm thick)</description>
      <identifier codeSpace="http://www.fao.org/global-soil-partnership/dictionary/GypsumFormsValue.xml">HL</identifier>
    </Definition>
