<?xml version='1.0' encoding='UTF-8'?>
<Dictionary xmlns="http://www.opengis.net/gml/3.2" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" gml:id="CarbonatesContentValue" xmlns:gml="http://www.opengis.net/gml/3.2" xsi:schemaLocation="http://www.opengis.net/gml/3.2 http://schemas.opengis.net/gml/3.2.1/gml.xsd">
  <identifier codeSpace="http://www.fao.org/global-soil-partnership/dictionary/CarbonatesContentValue.xml">Classification of carbonate reaction in the soil matrix</identifier>
  <dictionaryEntry>
    <Definition gml:id="CarbonatesContentValue_N">
      <description>0 Non-calcareous No detectable visible or audible effervescence.</description>
      <identifier codeSpace="http://www.fao.org/global-soil-partnership/dictionary/CarbonatesContentValue.xml">N</identifier>
    </Definition>
  </dictionaryEntry>
  <dictionaryEntry>
    <Definition gml:id="CarbonatesContentValue_SL">
      <description>≈ 0–2 Slightly calcareous Audible effervescence but not visible.</description>
      <identifier codeSpace="http://www.fao.org/global-soil-partnership/dictionary/CarbonatesContentValue.xml">SL</identifier>
    </Definition>
  </dictionaryEntry>
  <dictionaryEntry>
    <Definition gml:id="CarbonatesContentValue_MO">
      <description>≈ 2–10 Moderately calcareous Visible effervescence.</description>
      <identifier codeSpace="http://www.fao.org/global-soil-partnership/dictionary/CarbonatesContentValue.xml">MO</identifier>
    </Definition>
  </dictionaryEntry>
  <dictionaryEntry>
    <Definition gml:id="CarbonatesContentValue_ST">
      <description>≈ 10–25 Strongly calcareous Strong visible effervescence. Bubbles form a low foam.</description>
      <identifier codeSpace="http://www.fao.org/global-soil-partnership/dictionary/CarbonatesContentValue.xml">ST</identifier>
    </Definition>
  </dictionaryEntry>
  <dictionaryEntry>
    <Definition gml:id="CarbonatesContentValue_EX">
      <description>≈ &gt; 25 Extremely calcareous Extremely strong reaction. Thick foam forms quickly.</description>
      <identifier codeSpace="http://www.fao.org/global-soil-partnership/dictionary/CarbonatesContentValue.xml">EX</identifier>
    </Definition>
  </dictionaryEntry>
</Dictionary>
